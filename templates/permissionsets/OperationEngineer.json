{
    "Name": "OperationEngineer",
    "Description": "Operation tasks in AWS.",
    "SessionDuration": "PT4H",
    "ManagedPolicies": [
        "arn:aws:iam::aws:policy/job-function/ViewOnlyAccess"
    ],
    "CustomPolicy": {
        "Version": "2012-10-17",
        "Statement": [     
            {
                "Sid": "OperationRegularAccess",
                "Action": [
                    "account:ListRegions",
                    "guardduty:Get*",
                    "guardduty:List*",
                    "inspector2:Get*",
                    "inspector2:List*",
                    "inspector2:Batch*",
                    "fms:List*",
                    "fms:Get*",
                    "iam:Get*",
                    "iam:*CredentialReport",
                    "iam:CreateServiceLinkedRole",
                    "iam:PassRole",
                    "waf-regional:Get*",
                    "waf:Get*",
                    "wafv2:Get*",
                    "organizations:DescribeOrganization",                
                    "sso:List*",
                    "sso:Get*",
                    "sso:Describe*",
                    "sso:Search*",                
                    "securityhub:Get*",
                    "securityhub:List*",
                    "securityhub:Describe*"
                ],
                "Effect": "Allow",
                "Resource": "*"
            },         
            {
                "Sid": "OperationFullAccess",
                "Action": [
                    "aws-marketplace:*",
                    "account:*",
                    "acm:*",
                    "acm-pca:*",
                    "auditmanager:*",
                    "cloudformation:*",
                    "cloudhsm:*",
                    "cloudtrail:*",
                    "cloudwatch:*",
                    "cloudshell:*",
                    "codecommit:*",
                    "codedeploy:*",
                    "codepipeline:*",
                    "codestar:*",
                    "config:*",
                    "controltower:*",
                    "dynamodb:*",
                    "ds:*",
                    "events:*",
                    "ec2:*",
                    "eks:*",
                    "ecs:*",
                    "ecr:*",
                    "ecr-public:*",
                    "emr-containers:*",
                    "elasticloadbalancing:*",
                    "elasticmapreduce:*",
                    "elasticache:*",
                    "imagebuilder:*",
                    "identitystore:*",
                    "kms:*",
                    "kinesis:*", 
                    "firehose:*",
                    "lambda:*",
                    "mq:*",
                    "license-manager:*",
                    "logs:*",
                    "network-firewall:*",
                    "organizations:*",
                    "rds:*",
                    "ram:*",
                    "s3:*",
                    "sns:*",
                    "sqs:*",
                    "ses:*",
                    "secretsmanager:*",
                    "servicequotas:*",
                    "servicecatalog:*",
                    "ssm-contacts:*",
                    "serverlessrepo:*",                
                    "support:*",
                    "ssm:*",
                    "tag:*",
                    "trustedadvisor:*",
                    "wellarchitected:*"
                ],
                "Effect": "Allow",
                "Resource": "*"
            }     
        ]
    }
}