{
    "Name": "SecurityEngineer",
    "Description": "Security Engineer in AWS.",
    "SessionDuration": "PT4H",
    "ManagedPolicies": [
        "arn:aws:iam::aws:policy/job-function/ViewOnlyAccess",
        "arn:aws:iam::aws:policy/SecurityAudit"
    ],
    "CustomerManagedPolicies": [
        "IncidentResponse"
    ],
    "PermissionBoundary": {
        "PolicyType": "AWS",
        "Policy": "arn:aws:iam::aws:policy/job-function/SystemAdministrator"
    },       
    "CustomPolicy": {
        "Version": "2012-10-17",
        "Statement": [     
            {
                "Sid": "SecurityRegularAccess",
                "Action": [
                    "appstream:*Permission*",
                    "codeartifact:*Permissions*",
                    "cloudfront:*Policy*",
                    "ec2:*SecurityGroup*",
                    "ec2:*NetworkAcl*",
                    "ec2:*Snapshot*",
                    "ec2:*InstanceProfile*",
                    "ec2:*Permission*",
                    "ecr:DescribeImageScanFindings",
                    "ecr:StartImageScan",
                    "ecr:*Policy*",
                    "ecr:PutImageScanningConfiguration",
                    "ecr:PutRegistryScanningConfiguration",
                    "ecr:BatchGetRepositoryScanningConfiguration",
                    "ecr:GetRegistryScanningConfiguration",
                    "elasticache:*SecurityGroup*",
                    "elasticfilesystem:*SecurityGroup*",
                    "elasticloadbalancing:*SecurityGroup*",
                    "elasticloadbalancing:*Policy*",
                    "elasticmapreduce:*CreateSecurityConfiguration*",
                    "elasticmapreduce:*BlockPublicAccess*",
                    "elasticmapreduce:*Policy*",
                    "grafana:*Permission*",
                    "glue:*SecurityConfiguration*",
                    "glacier:*Policy*",
                    "iot:*SecurityProfile*",
                    "lakeformation:*Permission*",
                    "lambda:*Permission*",
                    "lambda:*Policy*",
                    "quicksight:*Permission*",
                    "s3:*PublicAccess*",
                    "s3:*Policy*",
                    "s3:*Acl*",
                    "s3:*Retention*",
                    "s3:*Owner*",
                    "sns:*Permission*",
                    "sqs:*Permission*"
                ],
                "Effect": "Allow",
                "Resource": "*"
            },         
            {
                "Sid": "SecurityFullAccess",
                "Action": [
                    "account:*",
                    "aws-marketplace:*",
                    "access-analyzer:*",
                    "acm:*",
                    "acm-pca:*",
                    "artifact:*",
                    "auditmanager:*",
                    "backup:*",
                    "cloudformation:*",
                    "cloudhsm:*",
                    "cloudtrail:*",
                    "cloudwatch:*",
                    "cloudshell:*",
                    "codecommit:*",
                    "codedeploy:*",
                    "codepipeline:*",
                    "codestar:*",
                    "cognito-identity:*",
                    "cognito-idp:*",
                    "cognito-sync:*",
                    "config:*",
                    "controltower:*",
                    "detective:*",
                    "ds:*",
                    "events:*",
                    "fms:*",
                    "firehose:*",
                    "guardduty:*",
                    "iam:*",
                    "identitystore:*",
                    "inspector:*",
                    "inspector2:*",
                    "imagebuilder:*",
                    "kinesis:*",
                    "kms:*",
                    "license-manager:*",
                    "logs:*",
                    "network-firewall:*",
                    "organizations:*",
                    "ram:*",
                    "secretsmanager:*",
                    "securityhub:*",
                    "servicequotas:*",
                    "servicecatalog:*",
                    "support:*",
                    "shield:*",
                    "ssm:*",
                    "sso:*",
                    "tag:*",
                    "trustedadvisor:*",
                    "waf-regional:*",
                    "waf:*",
                    "wafv2:*",
                    "wellarchitected:*"
                ],
                "Effect": "Allow",
                "Resource": "*"
            }
        ]
    }
}